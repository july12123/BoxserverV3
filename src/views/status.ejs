<!DOCTYPE html>
<head>
  <script src="/public/js/jquery.min.js"></script>
  <%- include('../partials/header'); %>
</head>

<navbar>
  <%- include('../partials/nav'); %>
</navbar>

<body>
<container class="container">
    <h1>Server Stats</h1>
    <div class="container">
        <h2>CPU</h2>
        <div class="progress">
        <div title="CPU" class="progress-bar" role="progressbar" aria-valuenow="50" aria-valuemin="0" aria-valuemax="100" style="width:0%" id="cpu">
            0%
        </div>
        </div>
    </div>
    <div class="container">
        <h2>Ram</h2>
        <div class="progress">
        <div title="Ram" class="progress-bar" role="progressbar" aria-valuenow="60" aria-valuemin="0" aria-valuemax="100" style="width:0%" id="ram">
            0%
        </div>
        </div>
    </div>
</container>
<div>
    <d>Server Time: </d><t id=time></t>
</div>
    <div>
    <d>Last Updated: </d><t id="updateTime"></t>
    </div>
    
<container class="container">
<table class="table">
    <thead>
        <tr>
        <th scope="col">Port</th>
        <th scope="col">Service</th>
        <th scope="col">Status</th>
        </tr>
    </thead>
    <tbody id=table>
    </tbody>
    </table>
</container>
<script src="/public/js/status.js"></script>
</body>

<%- include('../partials/js'); %>


</html>